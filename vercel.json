{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "buildCommand": "cd apps/web && npm install && npm run build",
    "outputDirectory": "apps/web/dist",
    "framework": null,
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/:path*"
        }
    ],
    "functions": {
        "apps/api/src/main.ts": {
            "memory": 1024,
            "maxDuration": 30
        }
    }
}